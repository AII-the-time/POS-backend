
model Store{
    id Int @id @default(autoincrement())
    user User @relation(fields: [userId], references: [id])
    userId Int
    name String
    address String
    defaultOpeningHours Json
    category Category[]
    menu Menu[]
    order Order[]
    preOrder PreOrder[]
    mileage Mileage[]
    option Option[]
    recipe Recipe[]
    stock Stock[]
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
